{
    "manifestVersion": 1,
    "id": "Multi-Pickers-Fields-Control",
    "version": "1.1.140",
    "name": "Multi Pickers Fields Control",
    "scopes": [
        "vso.code_full",
        "vso.work",
        "vso.work_write",
        "vso.identity",
        "vso.graph",
        "vso.project",
        "vso.security_manage"
    ],
    "description": "Pick value from filtered list",
    "publisher": "AviHadad",
    "icons": {
        "default": "img/logo.png"
    },
    "targets": [{
        "id": "Microsoft.VisualStudio.Services",
        "version": "[15.0,)"
    }],
    "tags": [
        "Work Item",
        "Work Item control",
        "Usability",
        "Picker",
        "Multi Pickers",
        "Multi List Values",
        "Storage",
        "List"
    ],
    "content": {
        "details": {
            "path": "details.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/avih75/PickerFieldsControl"
    },
    "links": {
        "repository": {
            "uri": "https://github.com/avih75/PickerFieldsControl"
        }
    },
    "files": [{
            "path": "img",
            "addressable": true
        },
        {
            "path": "scripts",
            "addressable": true
        },
        {
            "path": "styles",
            "addressable": true
        },
        {
            "path": "index.html",
            "addressable": true
        },
        {
            "path": "action.html",
            "addressable": true
        },
        {
            "path": "multiPickerConfig.html",
            "addressable": true
        }
    ],
    "categories": [
        "Plan and track"
    ],
    "contributions": [{
            "id": "Multi_Pickers_Fields_Control",
            "description": "Group values from several fields, and calculate new value. regarding the requested functionality",
            "type": "ms.vss-work-web.work-item-form-control",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Multi Pickers Fields with dependencies",
                "uri": "index.html",
                "height": 200,
                "inputs": [{
                        "id": "ControlName",
                        "name": "ControlName",
                        "description": "This field get the controll name, use for label and getting the infos from storage",
                        "type": "TextArea",
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    },
                    {
                        "id": "FieldName1",
                        "name": "FieldName1",
                        "description": "The first field name",
                        "type": "TextArea",
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    },
                    {
                        "id": "Field1",
                        "name": "Field1",
                        "description": "The first field workItem Field",
                        "type": "WorkItemField",
                        "properties": {
                            "workItemFieldTypes": [
                                "Integer",
                                "Double",
                                "String"
                            ]
                        },
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    },
                    {
                        "id": "FieldName2",
                        "name": "FieldName2",
                        "description": "The second field name",
                        "type": "TextArea",
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    },
                    {
                        "id": "Field2",
                        "name": "Field2",
                        "description": "The second field workItem Field",
                        "type": "WorkItemField",
                        "properties": {
                            "workItemFieldTypes": [
                                "Integer",
                                "Double",
                                "String"
                            ]
                        },
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    },
                    {
                        "id": "FieldName3",
                        "name": "FieldName3",
                        "description": "The therd field name",
                        "type": "TextArea",
                        "validation": {
                            "dataType": "String",
                            "isRequired": false
                        }
                    },
                    {
                        "id": "Field3",
                        "name": "Field3",
                        "description": "The therd field workItem Field",
                        "type": "WorkItemField",
                        "properties": {
                            "workItemFieldTypes": [
                                "Integer",
                                "Double",
                                "String"
                            ]
                        },
                        "validation": {
                            "dataType": "String",
                            "isRequired": false
                        }
                    },
                    {
                        "id": "FieldName4",
                        "name": "FieldName4",
                        "description": "The forth field name",
                        "type": "TextArea",
                        "validation": {
                            "dataType": "String",
                            "isRequired": false
                        }
                    },
                    {
                        "id": "Field4",
                        "name": "Field4",
                        "description": "The forth field workItem Field",
                        "type": "WorkItemField",
                        "properties": {
                            "workItemFieldTypes": [
                                "Integer",
                                "Double",
                                "String"
                            ]
                        },
                        "validation": {
                            "dataType": "String",
                            "isRequired": false
                        }
                    },
                    {
                        "id": "Reposetory",
                        "name": "Reposetory",
                        "description": "This field get the project name and the repository name, use for push the csv content to version control",
                        "type": "TextArea",
                        "validation": {
                            "dataType": "String",
                            "isRequired": false
                        }
                    }
                ]
            }
        },
        {
            "id": "myAction",
            "description": "Run an Upload file in work item actions menus",
            "type": "ms.vss-web.action",
            "targets": [
                "ms.vss-work-web.work-item-context-menu"
            ],
            "properties": {
                "text": "Upload Picker csv",
                "title": "Upload Picker control CSV Dependencies file",
                "icon": "img/icon.png",
                "groupId": "Picker",
                "uri": "action.html"
            }
        },
        {
            "id": "configureMultiPickerCSV",
            "type": "ms.vss-web.hub",
            "description": "Adds a 'Multi picker Fields Map Configuration' hub to the work Item configuration hub group.",
            "targets": [
                "ms.vss-web.collection-admin-hub-group"
            ],
            "properties": {
                "name": "MultiPicker",
                "uri": "multiPickerConfig.html"
            }
        }
    ]
}